  },
  "devDependencies": {
    "@tauri-apps/cli": "^1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.2.1",
    "prettier": "^3.2.5",
    "typescript": "^5.0.2",
    "vite": "^5.0.0"
  }
